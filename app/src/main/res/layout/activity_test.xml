<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
	<Button
	    android:id="@+id/registe_user"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="registe_user"/>
	
	<Button
	    android:id="@+id/registe_shop"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="registe_shop"/>
	
	<Button
	    android:id="@+id/edit_user_info"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="edit_user_info"/>
	
	<Button
	    android:id="@+id/shop_info"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="shop_info"/>
	
	<Button
	    android:id="@+id/login"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="login"/>
    
</LinearLayout>
